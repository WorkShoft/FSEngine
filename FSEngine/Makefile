ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild

SHELLTYPE := posix
ifeq (.exe,$(findstring .exe,$(ComSpec)))
	SHELLTYPE := msdos
endif

# Configurations
# #############################################

RESCOMP = windres
PCH = ../pch.h
PCH_PLACEHOLDER = $(OBJDIR)/$(notdir $(PCH))
GCH = $(PCH_PLACEHOLDER).gch
INCLUDES += -I.. -Isrc -I../deps/include -I/usr/local/lib64
FORCE_INCLUDE +=
ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
LIBS += -lGLEW -lSDL2 -lpugixml -lGL
LDDEPS +=
LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
define PREBUILDCMDS
endef
define PRELINKCMDS
endef

ifeq ($(config),debug)
TARGETDIR = ../bin/Debug-linux/FSEngine
TARGET = $(TARGETDIR)/libFSEngine.so
OBJDIR = ../bin/Debug-linux/FSEngine/int
DEFINES += -DFSE_DEBUG
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -fPIC -g
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -fPIC -g
ALL_LDFLAGS += $(LDFLAGS) -L../deps/lib -L/usr/lib64 -m64 -shared -Wl,-soname=libFSEngine.so
define POSTBUILDCMDS
	@echo Running postbuild commands
	cp -rf ../bin/Debug-linux/FSEngine/libFSEngine.so ../bin/Debug-linux/FSGame
endef

else ifeq ($(config),release)
TARGETDIR = ../bin/Release-linux/FSEngine
TARGET = $(TARGETDIR)/libFSEngine.so
OBJDIR = ../bin/Release-linux/FSEngine/int
DEFINES += -DFSE_RELEASE
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -O2 -fPIC
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -O2 -fPIC
ALL_LDFLAGS += $(LDFLAGS) -L../deps/lib -L/usr/lib64 -m64 -shared -Wl,-soname=libFSEngine.so -s
define POSTBUILDCMDS
	@echo Running postbuild commands
	cp -rf ../bin/Release-linux/FSEngine/libFSEngine.so ../bin/Release-linux/FSGame
endef

else ifeq ($(config),dist)
TARGETDIR = ../bin/Dist-linux/FSEngine
TARGET = $(TARGETDIR)/libFSEngine.so
OBJDIR = ../bin/Dist-linux/FSEngine/int
DEFINES += -DFSE_RELEASE -DFSE_DIST
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -O2 -fPIC
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -O2 -fPIC
ALL_LDFLAGS += $(LDFLAGS) -L../deps/lib -L/usr/lib64 -m64 -shared -Wl,-soname=libFSEngine.so -s
